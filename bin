local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

local scripts = {
    [3956818381] = "https://raw.githubusercontent.com/X-1Hz/1Hz/refs/heads/main/Ninja%20Legends",--Ninja Legens
