local a=loadstring(game:HttpGet'https://sirius.menu/rayfield')()
local b=a:CreateWindow{
Name="1 Hz ┃ Dead Rails",
Icon=0,
LoadingTitle="Loading...",
LoadingSubtitle="by 1 Hz",
Theme="Dark Blue",

DisableRayfieldPrompts=false,
DisableBuildWarnings=false,

ConfigurationSaving={
Enabled=true,
FolderName=nil,
FileName="by 1 Hz"
},

Discord={
Enabled=true,
Invite="YERMQTRRmQ",
RememberJoins=false
},

KeySystem=true,
KeySettings={
Title="1 Hz ┃ Dead Rails",
Subtitle="Key System",
Note="Join Discord For Get Key!",
FileName="Key",
SaveKey=false,
GrabKeyFromSite=true,
Key={"ILOVE_1Hz"}
}
}
a:Notify{
Title="1 Hz ┃ Dead Rails",
Content="by 1 Hz",
Duration=6.5,
Image="check",
}

print"Window created successfully!"


local c=game:GetService"RunService"
local d=workspace.CurrentCamera
local e=game:GetService"Players".LocalPlayer

local f={}
local g=RaycastParams.new()
g.FilterType=Enum.RaycastFilterType.Blacklist

local function isNPC(h)
return h:IsA"Model"
and h:FindFirstChild"Humanoid"
and h.Humanoid.Health>0
and h:FindFirstChild"Head"
and h:FindFirstChild"HumanoidRootPart"
and not game:GetService"Players":GetPlayerFromCharacter(h)
end

local function updateNPCs()
local h={}
for i,j in ipairs(workspace:GetDescendants())do
if isNPC(j)then
h[j]=true
end
end
for i=#f,1,-1 do
if not h[f[i] ]then
table.remove(f,i)
end
end
for i in pairs(h)do
if not table.find(f,i)then
table.insert(f,i)
end
end
end

local function handleDescendant(h)
if isNPC(h)then
table.insert(f,h)
local i=h:WaitForChild"Humanoid"
i.Destroying:Connect(function()
for j=#f,1,-1 do
if f[j]==h then
table.remove(f,j)
break
end
end
end)
end
end

workspace.DescendantAdded:Connect(handleDescendant)

local function predictPos(h)
local i=h:FindFirstChild"HumanoidRootPart"
local j=h:FindFirstChild"Head"
if not i or not j then
return j and j.Position or i and i.Position
end
local k=i.Velocity
local l=0.02
local m=i.Position+k*l
local n=j.Position-i.Position
return m+n
end

local function getTarget()
local h
local i=math.huge
local j=d.ViewportSize/2
g.FilterDescendantsInstances={e.Character}
for k,l in ipairs(f)do
local m=predictPos(l)
local n,o=d:WorldToViewportPoint(m)
if o and n.Z>0 then
local p=workspace:Raycast(
d.CFrame.Position,
(m-d.CFrame.Position).Unit*1000,
g
)
if p and p.Instance:IsDescendantOf(l)then
local q=(Vector2.new(n.X,n.Y)-j).Magnitude
if q<i then
i=q
h=l
end
end
end
end
return h
end

local function aim(h)
local i=d.CFrame
local j=(h-i.Position).Unit
local k=0.581
local l=i.LookVector:Lerp(j,k)
d.CFrame=CFrame.new(i.Position,i.Position+l)
end

local h=c.Heartbeat
local i=0
local j=0.4

local k=false

h:Connect(function(l)
i=i+l
if i>=j then
updateNPCs()
i=0
end
if k then
local m=getTarget()
if m then
local n=predictPos(m)
aim(n)
end
end
end)


workspace.DescendantRemoving:Connect(function(l)
if isNPC(l)then
for m=#f,1,-1 do
if f[m]==l then
table.remove(f,m)
break
end
end
end
end)


local l=b:CreateTab("Aimbot",4483362458)


l:CreateToggle{
Name="Enable Aimbot",
CurrentValue=false,
Flag="AimbotToggle",
Callback=function(m)
k=m
end
}


local m=b:CreateTab("Brings",4483362458)

local function GetItemNames()
local n={}
local o=workspace:FindFirstChild"RuntimeItems"
if o then
for p,q in ipairs(o:GetDescendants())do
if q:IsA"Model"then
table.insert(n,q.Name)
end
end
else
warn"RuntimeItems folder not found!"
end
return n
end

local n=m:CreateDropdown{
Name="Choose item",
Options=GetItemNames(),
CurrentOption="Select an item",
MultipleOptions=false,
Flag="ItemDropdown",
Callback=function(n)
if type(n)=="table"then
n=n[1]
end
end,
}

m:CreateButton{
Name="Refresh Items",
Callback=function()
n:Refresh(GetItemNames())
end,
}

m:CreateButton{
Name="Collect Selected Item",
Callback=function()
local o=n.CurrentOption
if type(o)=="table"then
o=o[1]
end

if o=="Select an item"then
warn"No item selected!"
return
end

local p=workspace:FindFirstChild"RuntimeItems"
if not p then
warn"RuntimeItems folder not found!"
return
end

local q
for r,s in ipairs(p:GetDescendants())do
if s:IsA"Model"and s.Name==o then
q=s
break
end
end

if not q then
warn("Item not found in RuntimeItems:",o)
return
end

local r=game:GetService"Players"
local s=r.LocalPlayer
if not s then
warn"LocalPlayer not found!"
return
end

local t=s.Character or s.CharacterAdded:Wait()
local u=t:WaitForChild"HumanoidRootPart"

if not q.PrimaryPart then
warn(q.Name.." has no PrimaryPart and cannot be moved.")
return
end

q:SetPrimaryPartCFrame(u.CFrame+Vector3.new(0,1,0))
print("Collected:",q.Name)
end,
}

m:CreateButton{
Name="Collect All Items",
Callback=function()
local o=workspace:FindFirstChild"RuntimeItems"
if not o then
warn"RuntimeItems folder not found!"
return
end

local p=game:GetService"Players".LocalPlayer
local q=p.Character or p.CharacterAdded:Wait()
local r=q:WaitForChild"HumanoidRootPart"

for s,t in ipairs(o:GetDescendants())do
if t:IsA"Model"then
if t.PrimaryPart then
local u=r.CFrame.LookVector*5
t:SetPrimaryPartCFrame(r.CFrame+u)
else
warn(t.Name.." has no PrimaryPart .")
end
end
end
end,
}

local o=b:CreateTab("ESP",4483362458)

local p={}
local q=false

local function CreateESP(r,s)
if not r or not r.PrimaryPart then return end

local t=Instance.new"Highlight"
t.Name="ESP_Highlight"
t.Adornee=r
t.FillColor=s
t.OutlineColor=s
t.Parent=r

local u=Instance.new"BillboardGui"
u.Name="ESP_Billboard"
u.Adornee=r.PrimaryPart
u.Size=UDim2.new(0,200,0,50)
u.StudsOffset=Vector3.new(0,5,0)
u.AlwaysOnTop=true
u.Parent=r

local v=Instance.new"TextLabel"
v.Text=r.Name
v.Size=UDim2.new(1,0,1,0)
v.TextColor3=s
v.BackgroundTransparency=1
v.TextSize=7
v.Parent=u

p[r]={Highlight=t,Billboard=u}
end

local function ClearESP()
for r,s in pairs(p)do
if s.Highlight then s.Highlight:Destroy()end
if s.Billboard then s.Billboard:Destroy()end
end
p={}
end

local function UpdateESP()
ClearESP()


local r=workspace:FindFirstChild"RuntimeItems"
if r then
for s,t in ipairs(r:GetDescendants())do
if t:IsA"Model"then
CreateESP(t,Color3.new(1,0,0))
end
end
end


local s=workspace:FindFirstChild"Baseplates"
if s and#s:GetChildren()>=2 then
local t=s:GetChildren()[2]
local u=t and t:FindFirstChild"CenterBaseplate"
local v=u and u:FindFirstChild"Animals"
if v then
for w,x in ipairs(v:GetDescendants())do
if x:IsA"Model"then
CreateESP(x,Color3.new(1,0,1))
end
end
end
end

local t=workspace:FindFirstChild"NightEnemies"
if t then
for u,v in ipairs(t:GetDescendants())do
if v:IsA"Model"then
CreateESP(v,Color3.new(0,0,1))
end
end
end

local u=workspace:FindFirstChild"RandomBuildings"and workspace.RandomBuildings:FindFirstChild"DestroyedHouse"
local v=u and u:FindFirstChild"StandaloneZombiePart"
local w=v and v:FindFirstChild"Zombies"
if w then
for x,y in ipairs(w:GetChildren())do
if y:IsA"Model"then
CreateESP(y,Color3.new(0,1,0))
end
end
end
end

local function AutoUpdateESP()
while q do
UpdateESP()
wait()
end
end

o:CreateToggle{
Name="ESP Items and Mobs",
CurrentValue=false,
Flag="ESPAllToggle",
Callback=function(r)
q=r
if r then
UpdateESP()
coroutine.wrap(AutoUpdateESP)()
else
ClearESP()
end
end
}

local r=b:CreateTab("Credits",4483362458)
r:CreateLabel"Join My discord For More Info!"
r:CreateButton{
Name="Copy Discord Link",
Callback=function()
setclipboard"https://discord.com/YERMQTRRmQ"
end,
}
